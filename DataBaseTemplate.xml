<?xml version="1.0" encoding="windows-1251" standalone="no" ?>

<root>
    <WorldPart>
        <Name Type="String"/>
    </WorldPart>
    <Country>
        <Name Type="String"/>
        <WorldPartID Type="Integer" Link="WorldPart"/>
    </Country>
    <Team>
        <Name Type="String"/>
        <CountryID Type="Integer" Link="Country"/>
        <IsNational Type="Bool"/>
    </Team>
    <Meet>
        <HomeTeamID Type="Integer" Link="Team"/>
        <AwayTeamID Type="Integer" Link="Team"/>
        <MainTimeGoalsFor Type="Integer" Default="0"/>
        <MainTimeGoalsAgainst Type="Integer" Default="0"/>
        <AddTimeGoalsFor Type="Integer" Default="0"/>
        <AddTimeGoalsAgainst Type="Integer" Default="0"/>
        <PenTimeGoalsFor Type="Integer" Default="0"/>
        <PenTimeGoalsAgainst Type="Integer" Default="0"/>
        <TournamentID Type="Integer" Link=""/>
        <StageID Type="Integer" Link=""/>
    </Meet>
    <Stage>
        <Name Type="String"/>
    </Stage>
</root>